<?xml version="1.0" encoding="utf-8"?>
<Application xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="Kouch.App.App" xmlns:values="clr-constants:Kouch.App.Constants" xmlns:behaviors="clr-constants:Kouch.App.Behaviors" xmlns:__xl="clr-namespace:__livexaml" __xl:Runtime.Register="C:\Users\Mitya\source\repos\Kouch.App\Kouch.App\Kouch.App\App.xaml">
  <Application.Resources>
    <Color x:Key="BlackColor">#000000</Color>
    <Color x:Key="ErrorColor">#ff5252</Color>
    <Style TargetType="Label">
      <Setter Property="TextColor" Value="Black"></Setter>
    </Style>
    <Style x:Key="Link" TargetType="Label">
      <Setter Property="TextDecorations" Value="Underline" />
      <Setter Property="TextColor" Value="Blue" />
      <Setter Property="FontSize" Value="15" />
    </Style>
    <Style x:Key="EntryStyle" TargetType="{x:Type Entry}">
      <Setter Property="behaviors:LineColorBehavior.ApplyLineColor" Value="True" />
      <Setter Property="behaviors:LineColorBehavior.LineColor" Value="{StaticResource BlackColor}" />
    </Style>
    <x:Array x:Key="registrationTemplates" Type="{x:Type x:String}">
      <x:String>phone</x:String>
      <x:String>code</x:String>
    </x:Array>
  </Application.Resources>
</Application>